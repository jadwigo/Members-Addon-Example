{
    "name": "jadwigo/wmmembertag",
    "description": "Wikimedia member tag profile addon",
    "type": "bolt-extension",
    "keywords": [
        "addon",
        "statistics",
        "wikimedia"
    ],
    "require": {
        "php": ">=5.5.9",
        "bolt/bolt": "^3.0",
        "bolt/members": "^3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Gawain Lynch",
            "email": "gawain.lynch@gmail.com"
        },
        {
            "name": "Lodewijk Evers",
            "email": "lodewijk@twokings.nl"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Jadwigo\\WmMemberTag\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\Jadwigo\\WmMemberTag\\Tests\\": "tests"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\Jadwigo\\WmMemberTag\\WmMemberTagExtension",
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
